{
    "editor.fontFamily": "Operator Mono Lig",
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "none",
    "explorer.confirmDragAndDrop": false,
    "diffEditor.renderSideBySide": true,
    "files.autoSave": "off",
    "editor.lineHeight": 24,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontFamily": "Operator Mono Lig",
    "terminal.integrated.fontWeight": 400,
    "editor.fontWeight": 400,
    "files.associations": {
        ".env": "php",
        "*.stub": "php"
    },
    "php-docblocker.qualifyClassNames": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.automationShell.linux": "",
    "window.zoomLevel": 1,
    "tailwindCSS.emmetCompletions": true,
    "editor.letterSpacing": 0,
    "blade.format.enable": true,
    "editor.inlineHints.fontFamily": "Operator Mono Lig",
    "editor.codeLensFontFamily": "Operator Mono Lig",
    "workbench.colorCustomizations": {
        "[Winter is Coming (Dark Blue)]": {
            "editorLineNumber.foreground": "#666",
            "gitDecoration.deletedResourceForeground": "#FF537090",
            "editorGutter.deletedBackground": "#FF537090",
            "gitDecoration.conflictingResourceForeground": "#FFCB6B90",
            "editorGutter.modifiedBackground": "#FFCB6B90",
            "gitDecoration.modifiedResourceForeground": "#82AAFF90",
            "gitDecoration.untrackedResourceForeground": "#C3E88D90",
            "editorGutter.addedBackground": "#82AAFF90",
            "gitDecoration.ignoredResourceForeground": "#676E9590",
            "terminal.ansiBlack": "#111525",
            "terminal.ansiBlue": "#a593c5",
            "terminal.foreground": "#fff",
        }
    },
    "workbench.colorTheme": "GitHub Light",
    "editor.fontSize": 14.4,
}
